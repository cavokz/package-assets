{
  "description": "Pipeline for setting event.ingested",
  "processors": [
    {
      "set": {
        "field": "event.ingested",
        "value": "{{ _ingest.timestamp }}",
        "ignore_failure": true
      }
    },
    {
      "pipeline": {
        "name": "metrics-endpoint.policy@custom",
        "ignore_missing_pipeline": true
      }
    }
  ],
  "_meta": {
    "managed_by": "fleet",
    "managed": true,
    "package": {
      "name": "endpoint"
    }
  }
}