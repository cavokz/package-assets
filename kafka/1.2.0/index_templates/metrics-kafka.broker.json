{
  "name": "metrics-kafka.broker",
  "index_template": {
    "index_patterns": [
      "metrics-kafka.broker-*"
    ],
    "template": {
      "settings": {
        "index": {
          "default_pipeline": "metrics-kafka.broker-1.2.0"
        }
      },
      "mappings": {
        "_meta": {
          "package": {
            "name": "kafka"
          },
          "managed_by": "fleet",
          "managed": true
        }
      }
    },
    "composed_of": [
      "metrics-kafka.broker@package",
      "metrics-kafka.broker@custom",
      ".fleet_globals-1",
      ".fleet_agent_id_verification-1"
    ],
    "priority": 200,
    "_meta": {
      "package": {
        "name": "kafka"
      },
      "managed_by": "fleet",
      "managed": true
    },
    "data_stream": {
      "hidden": false,
      "allow_custom_routing": false
    }
  }
}